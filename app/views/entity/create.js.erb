<%- else %><!-- TODO -->
  $("#add<%= @entity.from %>_errors").html( "<%= truncate(@response_message, :length => 253) %>");
  $("#add<%= @entity.from %>_errors").show();
  $("#form.new_<%= @entity_name %> input.<%= @entity_name %>_name").focus();
<%- end %>
